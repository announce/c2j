{
  "name": "cjt",
  "version": "1.1.1",
  "description": "A library to convert CSV text to Jira's table notation",
  "main": "index.js",
  "dependencies": {
    "javascript-csv": "^1.0.7",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "argparse": "^1.0.10",
    "mocha": "^5.2.0",
    "onchange": "^4.0.0",
    "power-assert": "^1.5.0",
    "standard": "^11.0.1"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "standard --env mocha --fix",
    "watch": "onchange '*.json' --delay 1000 'src/*' 'test/*' -- npm run build",
    "build": "npm run install && npm run lint && npm test",
    "start": "npm run build && npm run watch"
  },
  "standard": {
    "globals": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/announce/cjt.git"
  },
